# Teams Bot Configuration
BOT_ID=your-bot-app-id-here
BOT_PASSWORD=your-bot-password-here
BOT_ENDPOINT=https://your-domain.com/api/messages

# Your OnPrem WebSocket Configuration
WEBSOCKET_URL=ws://your-onprem-server:port/websocket
WEBSOCKET_PROTOCOL=ws

# Server Configuration
PORT=3978
NODE_ENV=development

# Teams App Configuration
TEAMS_APP_ID=your-teams-app-id-here
TEAMS_APP_PASSWORD=your-teams-app-password-here

# Audio Configuration
AUDIO_SAMPLE_RATE=16000
AUDIO_CHANNELS=1
AUDIO_BIT_DEPTH=16

# Logging
LOG_LEVEL=info
LOG_FILE=logs/bot.log

# Security
ENCRYPTION_KEY=your-32-character-encryption-key-here

# Optional: Azure Bot Service (if using)
AZURE_BOT_SERVICE_ENDPOINT=https://your-bot-service.azurewebsites.net

# Optional: Ngrok for local development
NGROK_URL=https://your-ngrok-url.ngrok.io 